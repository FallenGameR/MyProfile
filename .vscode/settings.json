{
    "cSpell.enableFiletypes": [
        "powershell"
    ],
    "cSpell.ignorePaths": [
        "package-lock.json",
        "node_modules",
        "vscode-extension",
        ".git/objects",
        ".vscode",
        ".vscode-insiders",
        "*-apps.txt",
    ],
    "cSpell.words": [
        "adsi",
        "Aleksandr",
        "ALEXKO",
        "apgold",
        "APPDATA",
        "AUTOPILOTHUB",
        "azcompute",
        "batcat",
        "bing",
        "chafa",
        "choco",
        "COMPUTERNAME",
        "Conhost",
        "datetime",
        "dconf",
        "Distrib",
        "DWORD",
        "fallengamer",
        "hacky",
        "hardcoded",
        "hashtable",
        "HKCU",
        "inetpub",
        "inetroot",
        "Jupyter",
        "Kostikov",
        "LASTEXITCODE",
        "ldap",
        "linenumber",
        "Linq",
        "Lucida",
        "mkdir",
        "mklink",
        "MSRC",
        "Multitran",
        "noprofile",
        "notcontains",
        "notmatch",
        "onebranch",
        "opensource",
        "Passthru",
        "pfgold",
        "Prepend",
        "prorab",
        "psitem",
        "psobject",
        "psreadline",
        "pstoolset",
        "pwsh",
        "Readline",
        "repos",
        "Retryable",
        "RIPGREP",
        "RRGGBB",
        "rundll",
        "Sekirei",
        "steppable",
        "Sysinternals",
        "timespan",
        "Toolset",
        "toplevel",
        "totalcmd",
        "tracert",
        "USERDOMAIN",
        "USERPROFILE",
        "Winamp",
        "winget",
        "xattr",
        "xcomm",
        "xelem",
        "xname",
        "xsel"
    ],
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/Help": true,
        "**/.DS_Store": true,
    },
    "powershell.codeFormatting.openBraceOnSameLine": false,
    "powershell.codeFormatting.pipelineIndentationStyle": "IncreaseIndentationForFirstPipeline",
    "powershell.codeFormatting.useCorrectCasing": true,
    "powershell.codeFormatting.whitespaceBeforeOpenBrace": false,
    "powershell.codeFormatting.whitespaceBeforeOpenParen": false,
    "powershell.codeFormatting.whitespaceInsideBrace": false,
    "powershell.codeFormatting.alignPropertyValuePairs": false
}